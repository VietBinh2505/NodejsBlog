<% include ./../../helpers/show-box-list-invitation %>
<%
    if (typeof userData.totalreq === 'undefined') userData.totalreq = 0;
%>
<div class="box box-primary one-box" id="notify-receive">
    <div class="box-header with-border">
        <h3 class="box-title">Danh sách lời mời gửi đi</h3>
        <span class="badge bg-light-blue count" ><%= userData.totalreq %></span>
    </div>
    <div class="box-body">
        <%- showBoxListInvitation(userData.totalreq, userData.reqFrom, "receive") %>
    </div>
</div>